@using Webdiyer.WebControls.Mvc
@using IYun.Models;
@model PagedList<YD_Edu_StuState>

<table class="xx-table ydong-tab">
    <tr>
        <th>学籍状态</th>
        <th>学籍状态代码</th>
        <th>操作</th>
    </tr>
    @{
        foreach (var ydSysAdmin in Model)
        {
            <tr>
                <td>@ydSysAdmin.y_name</td>
                <td>@ydSysAdmin.y_stuStateCode</td>
                <td><a class="bfont2" href="/DicAdmin/StuStateEditPage/@ydSysAdmin.id">修改</a><a class="bfont2" href="javascript:deleteStuState(@ydSysAdmin.id)">删除</a></td>
            </tr>
        }
    }
</table>

