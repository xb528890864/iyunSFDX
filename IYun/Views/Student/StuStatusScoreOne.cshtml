        if (ydSysSubSchool != null)
        {
            changetag = ydSysSubSchool.y_name;
        }
    }
    var changetagB = "";
    if (strange.y_strangeType == 1)
    {
        var majorid = Convert.ToInt32(strange.y_contentB);
        var ydEduMajor = new IYunEntities().YD_Edu_Major.Find(majorid);
