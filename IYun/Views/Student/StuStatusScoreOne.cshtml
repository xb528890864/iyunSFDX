    var changetag = "";
    if (strange.y_strangeType == 1)
    {
        var majorid = Convert.ToInt32(strange.y_contentA);
        var ydEduMajor = new IYunEntities().YD_Edu_Major.Find(majorid);
        if (ydEduMajor != null)
        {
            changetag = ydEduMajor.y_name;
