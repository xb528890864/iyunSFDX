@using Webdiyer.WebControls.Mvc
@using System.Configuration
@using IYun.Models;
@using IYun.Common
@model PagedList<YD_Edu_Major>
@{
    int adminid = ViewBag.adminid;
    var schoolname = ConfigurationManager.AppSettings["SchoolName"];
}
<table class="xx-table ydong-tab">
    <tr>
       
        <th>专业</th>  
        <th>层次</th>    
        <th>培养形式</th>    
        <th>代码</th>
        <th>专业类型</th>
        <th>学制</th>
        <th>操作</th>
    </tr>
    @{
        foreach (var ydSysAdmin in Model)
        {
            <tr>
                <td>@ydSysAdmin.YD_Edu_MajorLibrary.y_name</td>   
                <td>@ydSysAdmin.YD_Edu_EduType.y_name</td>    
                <td>@ydSysAdmin.YD_Edu_StuType.y_name</td>    
                <td>@ydSysAdmin.y_code</td>
                <td>@ydSysAdmin.y_majortype</td>
                <td>@ydSysAdmin.y_stuYear</td>

                <td><a class="bfont2" href="/basicdata/MajorEditPage/@ydSysAdmin.id">修改</a><a class="bfont2" href="javascript:deleteSubSchool(@ydSysAdmin.id)">删除</a></td>
            </tr>
        }
    }
</table>
