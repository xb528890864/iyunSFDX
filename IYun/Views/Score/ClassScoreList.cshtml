    <script type="text/javascript">
        //EvPNG.fix('div, ul, img, li, input');

        $(function () {
            if (window.PIE) {
                $('.rounded').each(function () {
                    PIE.attach(this);
                });
            }
        });

        function myPrint(obj) {
            var newWindow = window.open("/Score/ClassScoreListPrint", "_blank");//打印窗口要换成页面的url
            $("#prev").hide();
            var docStr = obj.innerHTML;
            newWindow.document.write(docStr);
            newWindow.document.close();
            newWindow.print();
            newWindow.close();
            $("#prev").show();
        }

        function yanzheng() {
            if ($('#EnrollYear2').val() == 0) {
                alert("请选择入学年份");
                return false;
            }
            if ($('#SubSchool').combobox('getValue') == 0) {
                alert("请选择函授站");
                return false;
            }
            if ($('#Major').combobox('getValue') == 0) {
                alert("请选择专业");
                return false;
            }
            return true;
        }
        function allScoreOK() {
            if (yanzheng) {
                var rows = $('#StuForm').serializeObject();
                var ids = "";
                if (!Array.isArray(rows.id)) {
                    ids = rows.id;
                }
                else {
                    for (var i = 0; i < rows.id.length; i++) {
                        ids += rows.id[i];
                        if (i + 1 < rows.id.length) {
                            ids += ",";
                        }
                    }
                }
                $.ajax({
                    url: '@Url.Action("ClassScoreOk", "TeaPlan")',
                    data: { Ids: ids },
                        type: 'post',
                        dataType: 'json',
                        success: function(data) {
                            if (data.status == "ok") {
                                alert(data.msg);
                            } else {
                               alert(data.msg);
                            }
                        }
                    });
            }
        }
        function allRollback() {
            if (yanzheng) {
                var rows = $('#StuForm').serializeObject();
                var ids = "";
                if (!Array.isArray(rows.id)) {
                    ids = rows.id;
                }
                else {
                    for (var i = 0; i < rows.id.length; i++) {
                        ids += rows.id[i];
                        if (i + 1 < rows.id.length) {
                            ids += ",";
                        }
                    }
                }

                 $.ajax({
                        url: '@Url.Action("ClassAllRollback", "TeaPlan")',
                        data: { Ids: ids },
                        type: 'post',
                        dataType: 'json',
                        success: function(data) {
                            if (data.status == "ok") {
                                alert(data.msg);
                            } else {
                               alert(data.msg);
                            }
                        }
                    });
            }
        }
        function scoreOk(id) {
            if (yanzheng) {
                 $.ajax({
                    url: '@Url.Action("ScoreOk", "TeaPlan")',
                    data: { Id: id },
                    type: 'post',
                    dataType: 'json',
                    success: function(data) {
                        if (data.status == "ok") {
                            alert(data.msg);
                            location.reload();
                        } else {
                            alert(data.msg);
                        }
                    }
                });
            }
        }

        function scoreBack(id) {
            if (yanzheng) {
                 $.ajax({
                    url: '@Url.Action("ScoreBack", "TeaPlan")',
                    data: {id:id},
                    type: 'post',
                    dataType: 'json',
                    success: function(data) {
                        if (data.status == "ok") {
                            alert(data.msg);
                            location.reload();
                        } else {
                            alert(data.msg);
                        }
                    }
                });
            }
        }
    </script>
