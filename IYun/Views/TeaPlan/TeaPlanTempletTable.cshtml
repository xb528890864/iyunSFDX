@using Webdiyer.WebControls.Mvc
@using IYun.Models;
@model PagedList<YD_TeaPlan_Templet>

<table class="xx-table ydong-tab">
    <tr>
        <th>专业名称</th>
        <th>层次</th>
        <th>学习形式</th>
        <th>教学计划</th>
    </tr>
    @{
        foreach (var item in Model)
        {
            <tr>
                <td>@item.YD_Edu_Major.YD_Edu_MajorLibrary.y_name</td>
                <td>@item.YD_Edu_Major.YD_Edu_EduType.y_name</td>
                <td>@item.YD_Edu_Major.YD_Edu_StuType.y_name</td>
                <td><a class="bfont2" target="_blank" href="@Url.Action("TeaPlanTempletDes", "TeaPlan", new{item.id})">查看教学计划</a></td>
            </tr>
        }
    }
</table>
