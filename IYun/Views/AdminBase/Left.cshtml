@using IYun.Controllers
@using IYun.Models
@{
    Layout = null;
}

<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <title>南昌师范学院后台</title>
    <meta name="description" content="overview &amp; stats" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    
    
    <script src="~/Content/IYun/js/jquery-1.9.1.min.js"></script>
   
    <script>
        function jumpRight(url, moduleName) {
            var frames = window.parent.window.document.getElementById("main");
            frames.contentWindow.addTabs(url, moduleName);
        }
    </script>
</head>
<body class="no-skin">
    <!--主框架开始-->
    <div class="main-container" id="main-container">
        <!-- 左侧栏目开始 -->
        <div id="sidebar" class="sidebar responsive" style="height:100%; background:#F8F8F8;">
            <script type="text/javascript">
                try { ace.settings.check('sidebar', 'fixed') } catch (e) { }
            </script>

            <!--左侧栏目子菜单列表开始-->
            <ul class="nav nav-list">
                <li class="active">
                    <!--class="active"代表该菜单栏处于被选中状态-->
                    <a href="javascript:jumpRight('/Base/Board','统计面板')" target="main">
                        <i class="menu-icon fa fa-tachometer"></i>
                        <span class="menu-text">主页面板 </span>
                    </a>
                    <b class="arrow"></b>
                </li>
                @{
                    var modulePowers = (List<ModulePower>)ViewBag.modulePowers;
                    @AdminBaseController.GetModule(modulePowers)
                }

            </ul>
            <!--左侧栏目子菜单列表结束-->
        </div>
        <!-- 左侧栏目结束 -->

    </div>
    <!--主框架结束-->

</body>
</html>
